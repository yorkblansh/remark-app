{
	"name": "scripts",
	"private": true,
	"version": "0.0.1",
	"author": "Oleg Chepelev <yorkblansh@gmail.com>",
	"module": "2022",
	"scripts": {
		"script:update-api-by-forms": "ts-node ./src/update-api-by-forms",
		"script:create-form-template": "ts-node ./src/create-form-template",
		"script:generate-jsx-forms-map": "ts-node ./src/generate-jsx-forms-map",
		"script:generate-static-temlates": "ts-node ./src/generate-static-temlates",
		"script:remove-temp-folders": "LIB_NAME=remove-temp-folders vite build && cross-env LIB_NAME=remove-temp-folders npm run do-script",
		"script:generate-form-interfaces": "LIB_NAME=generate-form-interfaces vite build && cross-env LIB_NAME=generate-form-interfaces npm run do-script",
		"script:generate-form-enum": "ts-node ./src/generate-form-enum",
		"script:symlink-shared": "LIB_NAME=symlink-shared vite build && cross-env LIB_NAME=symlink-shared npm run do-script",
		"script:symlink-forms": "LIB_NAME=symlink-forms vite build && cross-env LIB_NAME=symlink-forms npm run do-script",
		"do-script": "cross-env-shell \"node ./dist/$LIB_NAME/$LIB_NAME\"",
		"prebuild": "rimraf dist",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main.mjs",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@vitejs/plugin-react": "^3.1.0",
		"async": "^3.2.4",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"fp-ts": "^2.13.1",
		"lodash.chunk": "^4.2.0",
		"lodash.debounce": "^4.0.8",
		"lodash.last": "^3.0.0",
		"lodash.throttle": "^4.1.1",
		"symlink-or-copy": "^1.3.1",
		"ts-morph": "^17.0.1",
		"ts-node": "^10.9.1"
	},
	"devDependencies": {
		"@swc/core": "^1.3.11",
		"@types/async": "^3.2.15",
		"@types/express": "^4.17.14",
		"@types/lodash.chunk": "^4.2.7",
		"@types/lodash.debounce": "^4.0.7",
		"@types/lodash.last": "^3.0.7",
		"@types/lodash.throttle": "^4.1.7",
		"@types/node": "^18.13.0",
		"@types/supertest": "^2.0.12",
		"@types/symlink-or-copy": "^1.2.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.1",
		"typescript": "^4.9.5",
		"vite": "^3.2.2",
		"vite-plugin-node": "^1.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"packageManager": "yarn@3.2.4",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
